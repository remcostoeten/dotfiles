# Copy to clipboard function (handles both X11 and Wayland)
copy_to_clipboard() {
    local emoji="$1"
    if command -v wl-copy &> /dev/null && [ -n "$WAYLAND_DISPLAY" ]; then
        echo -n "$emoji" | wl-copy
        echo -e "${GREEN}âœ“${NC} Copied to clipboard (Wayland): ${CYAN}$emoji${NC}"
    elif command -v xclip &> /dev/null; then
        echo -n "$emoji" | xclip -selection clipboard
        echo -e "${GREEN}âœ“${NC} Copied to clipboard (X11): ${CYAN}$emoji${NC}"
    else
        echo -e "${RED}âœ—${NC} Neither xclip nor wl-copy found. Please install one of them."
        echo -e "${YELLOW}Install with:${NC} sudo apt install xclip ${YELLOW}or${NC} sudo apt install wl-clipboard"
        return 1
    fi
}

